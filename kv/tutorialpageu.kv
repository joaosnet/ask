<TutorialPage1>:
    FloatLayout:
        BoxLayout:
            pos_hint: {"right": 1, "top": 0.75}
            size_hint: 1, 0.63
            orientation:'vertical'
            id: areatutorial
        # Barra de Navegação Inferior
        GridLayout:
            row: 1
            cols: 3
            pos_hint: {"right": 1, "top": 0.12}
            size_hint: 1, 0.12
            canvas:
                Color:
                    rgb: 1,1,1,1 # cor branca
                Rectangle:
                    pos: self.pos
                    size: self.size
            # botão adionar venda
            MDLabel:
                text: "Pular"
                color: 0,0,0,1 # cor preta
                font_style: "Caption"
                halign: "center"
                valign: "center"
                font_size: 25

            # botão lista de vendedores
            ImageButton:
                source: "icones/Meatballs_menu.png"
            # botão voltar
            ImageButton:
                source: "icones/proximo_roxo.png"
                on_release: app.mudar_tela("tutorialpaged")
